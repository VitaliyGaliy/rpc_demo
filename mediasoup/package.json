{
  "name": "mediasoup",
  "version": "1.0.0",
  "description": "mediasoup",
  "repository": "https://github.com/anovikov1984/remotepc_meet",
  "main": "mediasoupServer.ts",
  "author": "Codeda",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/node": "^10.17.16",
    "browserify": "^16.5.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "console-stamp": "^0.2.9",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "https": "^1.0.0",
    "mediasoup": "^3.4.11",
    "mediasoup-client": "^3.3.17",
    "moment": "^2.24.0",
    "router": "^1.3.4"
  },
  "scripts": {
    "tsc": "cd front;tsc",
    "browserify": "browserify ./front/dist/front/src/index.js -o ./front/dist/index.js",
    "build": "npm run tsc;npm run browserify",
    "test": "ts-node ./mediasoupServer.ts  >~/mediasoupServer.log 2>&1 &",
    "service": "ts-node ./mediasoupServer.ts  >~/mediasoupServer.log 2>&1",
    "start": "ts-node ./mediasoupServer.ts",
    "kill": "ps -ef | grep mediasoupServer.ts | grep -v grep | awk '{print $2}' | xargs kill -9",
    "copylog": "if [ -f ~/mediasoupServer.log ]; then cp ~/mediasoupServer.log ~/mediasoupServer-$(date +%Y-%m-%d-%H-%M).log; fi",
    "restart": "npm run kill;npm run copylog;npm start",
    "killffmpegs": "ps -ef | grep ffmpeg | grep -v grep | awk '{print $2}' | xargs kill -9"
  }
}
